{
  "name": "amino-acid-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm start",
    "install-client": "cd client && npm install",
    "postinstall": "npm run install-client",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run client\"",
    "test": "mocha --exit"
  },
  "dependencies": {
    "express": "^4.17.1",
    "cors": "^2.8.5",
    "mongoose": "^6.1.1",
    "cookie-parser": "^1.4.5"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "mocha": "^9.0.0",
    "supertest": "^6.1.3"
  }
}
